{
    "name": "react-directory-lister",
    "version": "0.1.0",
    "description": "Simple directory lister built with React, Flux, Webpack, and Express",
    "scripts": {
        "build": "webpack -p --progress",
        "start-api": "node api-server.js",
        "start-webpack-server": "webpack-dev-server --content-base . --inline --hot",
        "dev": "concurrently --kill-others \"npm run start-webpack-server\" \"npm run start-api\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mfrazi/react-directory-lister"
    },
    "author": {
        "name": "Muhammad Fahrul Razi",
        "email": "iam@mfrazi.me"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mfrazi/react-directory-lister/issues"
    },
    "devDependencies": {
        "babel-core": "^6.0.20",
        "babel-loader": "^6.0.1",
        "babel-preset-es2015": "^6.0.15",
        "babel-preset-react": "^6.0.15",
        "concurrently": "^2.2.0",
        "webpack": "^1.12.2",
        "webpack-dev-server": "^1.12.1",
        "axios": "^0.14.0",
        "flux": "^2.1.1",
        "react": "^15.3.1",
        "react-dom": "^15.3.1"
    },
    "dependencies": {
        "express": "^4.14.0"
    }
}
